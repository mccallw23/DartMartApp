{
  "expo": {
    "name": "project-dartmart",
    "slug": "project-dartmart",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./src/assets/DartMartLogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.manuwill.projectdartmart",
      "infoPlist": {
         "NSCameraUsageDescription": "To scan credit cards.",
         "ITSAppUsesNonExemptEncryption ": false
       }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "638baced-7612-4f56-9c81-09d21165b73f"
      }
    }
  }
}
